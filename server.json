{
  "app": {
    "plugins" :{
      "router": {
        "module": "/plugins/router.js",
        "options": {}
      },
      "geoip": {
        "module": "/plugins/geoip.js",
        "options": {
          "db": "/usr/local/share/GeoIP/GeoIPCity.dat",
          "additional_dbs": [
            "/usr/local/share/GeoIP/GeoIPCity_1.dat",
            "/usr/local/share/GeoIP/GeoIPCity_2.dat",
            "/usr/local/share/GeoIP/GeoIPCity_3.dat",
            "/usr/local/share/GeoIP/GeoIPCity_4.dat"
          ],
          "couchDBCradle": {
            "local": {
              "dbUrl": "couchdb-alpha.vast.com",
              "dbPort": 5984,
              "extraConf": {},
              "dbName": "geolocation_logs",
              "lookupsTotalCount": "lookups_total_count",
              "lookupsFailedCount": "lookups_failed_count",
              "lookupsOlderDbsCount": "lookups_older_dbs_count"
            },
            "alpha": {
              "dbUrl": "couchdb-alpha.vast.com",
              "dbPort": 5984,
              "extraConf": {},
              "dbName": "geolocation_logs",
              "lookupsTotalCount": "lookups_total_count",
              "lookupsFailedCount": "lookups_failed_count",
              "lookupsOlderDbsCount": "lookups_older_dbs_count"
            },
            "beta": {
              "dbUrl": "couchdbbeta.vast.com",
              "dbPort": 5984,
              "extraConf": { "auth": {"username": "mixdown", "password": "r4sYSlaDee74H4C"}},
              "dbName": "geolocation_logs",
              "lookupsTotalCount": "lookups_total_count",
              "lookupsFailedCount": "lookups_failed_count",
              "lookupsOlderDbsCount": "lookups_older_dbs_count"
            },
            "prod": {
              "dbUrl": "couchdbprod.vast.com",
              "dbPort": 5984,
              "extraConf": { "auth": {"username": "mixdown", "password": "r4sYSlaDee74H4C"}},
              "dbName": "geolocation_logs",
              "lookupsTotalCount": "lookups_total_count",
              "lookupsFailedCount": "lookups_failed_count",
              "lookupsOlderDbsCount": "lookups_older_dbs_count"
            }
          },
          "debug": false
        }
      },
      "json": {
        "module": "mixdown-plugins#Json",
        "options": {
          "jsonpEnabled": true
        }
      },
      "error": {
        "module": "mixdown-plugins#Error"
      },
      "metrics": {
        "module": "vast-plugins#Metrics",
        "options": {
            "port": 8125,
            "host": "sitemap.vast.com"
        }
      }
    }
  },
  "sites": [
    {
      "id": "geoip",
      "hostmap": [
        "localhost",
        "127.0.0.1",
        "geoip.fe-local.vast.com",
        "geoip.fe-alpha.vast.com",
        "beta-geoip-node.vast.com",
        "geoip-node.vast.com"
      ]
    }
  ],
  "server": {
    "listen": {
      "port": 9002,
      "type": "tcp",
      "path": "/tmp/geoip.sock"
    },
    "logger": {
      "defaults": {
        "handleExceptions": false,
        "json": false,
        "timestamp": true,
        "colorize": false,
        "prettyPrint": false
      },
      "transports": [{
        "transport": "Syslog",
        "options": {
          "handleExceptions": true,
          "facility": "local1",
          "level": "info",
          "host": "localhost",
          "port": 514,
          "app_name": "geoip",
          "pid": null
        }
      }]
    }
  }
}